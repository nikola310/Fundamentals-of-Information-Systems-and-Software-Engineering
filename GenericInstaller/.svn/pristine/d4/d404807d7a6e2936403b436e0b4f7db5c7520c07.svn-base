package model.tree;

import java.io.Serializable;

public class FinishModel implements Serializable {

	/*
	 * Model za poslednji panel, sadrzi tekst koji ce se prikazati, ime readme i
	 * izvrsne datoteke, te putanju do ikone
	 */
	private static final long serialVersionUID = 1L;
	private String description;
	private String readme;
	private String exe;
	private String icon;

	public FinishModel() {
		description = "";
		readme = "";
		exe = "";
		icon = "Icon.jpg";
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public String getReadme() {
		return readme;
	}

	public void setReadme(String readme) {
		this.readme = readme;
	}

	public String getExe() {
		return exe;
	}

	public void setExe(String exe) {
		this.exe = exe;
	}

	public String getIcon() {
		return icon;
	}

	public void setIcon(String icon) {
		this.icon = icon;
	}

	@Override
	public String toString() {
		return "FinishModel [description=" + description + ", readme=" + readme
				+ ", exe=" + exe + ", icon=" + icon + "]";
	}

}
