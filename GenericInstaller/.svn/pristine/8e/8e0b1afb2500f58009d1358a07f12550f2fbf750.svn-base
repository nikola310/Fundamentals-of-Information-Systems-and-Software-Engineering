package model.parameters;

import java.io.Serializable;
import java.util.ArrayList;

import javax.swing.JTextField;

public class SelectionModel implements Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private boolean type;
	private ArrayList<JTextField> choicesList;
	private String label;
	
	public boolean isType() {
		return type;
	}

	public void setType(boolean type) {
		this.type = type;
	}

	public ArrayList<JTextField> getChoicesList() {
		return choicesList;
	}

	public void setChoicesList(ArrayList<JTextField> choicesList) {
		this.choicesList = choicesList;
	}

	public String getLabel() {
		return label;
	}

	public void setLabel(String label) {
		this.label = label;
	}

	public SelectionModel(boolean type, ArrayList<JTextField> choicesList, String label) {
		super();
		this.type = type;
		this.choicesList = choicesList;
		this.label = label;
	}

}
